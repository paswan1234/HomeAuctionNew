{
  "name": "auction",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "vite",
    "start:dev": "vite --mode dev",
    "build": "vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "commit": "cz",
    "release": "standard-version",
    "folderslint": "folderslint",
    "lint-scss": "stylelint \"**/*.{scss,css}\"",
    "lint": "eslint --ignore-pattern \"src/reportWebVitals.js\" \"src/**/*.{js,jsx,json}\" --fix",
    "format": "prettier \"src/**/*.{js,jsx,json,css,scss,md}\" --write"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "bootstrap": "^5.2.0",
    "folderslint": "^1.2.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-sass": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-bootstrap-typeahead": "6.1.2",
    "react-dom": "^18.2.0",
    "react-owl-carousel": "^2.3.3",
    "react-paginate": "8.2.0",
    "react-router-dom": "^6.4.1",
    "react-select": "^5.7.3",
    "react-slick": "^0.30.1",
    "stylelint": "^15.3.0",
    "stylelint-scss": "^4.5.0",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-css": "^0.6.0",
    "eslint-plugin-folders": "^1.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.60.0",
    "standard-version": "^9.5.0",
    "vite": "^4.2.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,json}": [
      "npm run lint",
      "npm run format",
      "npm run folderslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
