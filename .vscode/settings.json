{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#88e4da",
    "activityBar.activeBorder": "#046354",
    "activityBar.background": "#88e4da",
    "activityBar.foreground": "#5B640D",
    "activityBar.inactiveForeground": "#5B640D",
    "activityBarBadge.background": "#6F6C63",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#88e4da",
    "statusBar.background": "#ADB82D",
    "statusBar.foreground": "#6F6C63",
    "statusBarItem.hoverBackground": "#ceb074",
    "statusBarItem.remoteBackground": "#ADB82D",
    "statusBarItem.remoteForeground": "#6F6C63",
    "titleBar.activeBackground": "#ADB82D",
    "titleBar.activeForeground": "#6F6C63",
    "titleBar.inactiveBackground": "#ADB82D99",
    "titleBar.inactiveForeground": "#6F6C6399"
  },
  "editor.tabSize": 2,
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "files.trimTrailingWhitespace": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "diffEditor.ignoreTrimWhitespace": false,
  "emmet.syntaxProfiles": {
    "javascript": "js"
  },
  "emmet.triggerExpansionOnTab": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
